{
  "name": "angular2-rollup",
  "version": "4.3.0-beta.0",
  "license": "MIT",
  "repository": "https://github.com/steveblue/angular2-rollup",
  "contributors": [
    "Stephen Belovarich",
    "Jason Aden",
    "Felix Itzenplitz",
    "Kyle Cordes",
    "olegreznichenko"
  ],
  "scripts": {
    "clean": "rimraf node_modules ngfactory doc build && npm cache clean",
    "clean:install": "npm run clean && npm install",
    "clean:build": "rimraf build",
    "clean:tmp": "rimraf tmp",
    "clean:ngfactory": "rimraf ngfactory && mkdir ngfactory",
    "clean:lib": "rm ./dist/index.ts",
    "compile:prod": "ngc -p ./tsconfig.prod.json",
    "compile:lib": "ngc -p ./tsconfig.lib.json",
    "compile:es5": "ngc -p ./tsconfig.lib.es5.json",
    "compile:umd": "ngc -p ./tsconfig.lib.es5.json",
    "copy:lib": "rsync -a --exclude=*.js ngfactory/ dist",
    "copy:package": "cp ./src/lib/package.json ./dist/package.json",
    "bundle:prod": "rollup -c rollup.config.js",
    "rollup:lib": "rollup -c rollup.config.lib.js",
    "rollup:es5": "rollup -c rollup.config.lib-es5.js",
    "rollup:umd": "rollup -c rollup.config.lib-umd.js",
    "transpile:vendor": "tsc --out ./build/vendor.js --target es5 --allowJs build/vendor.es2015.js || true",
    "transpile:src": "tsc -p ./tsconfig.dev.json",
    "transpile:prod": "java -jar node_modules/google-closure-compiler/compiler.jar --warning_level=QUIET --language_in=ES6 --language_out=ES5 --js ./build/bundle.es2015.js --js_output_file ./build/bundle.js",
    "transpile:lib": "babel --presets=es2015-rollup ./dist/default-lib.es5.js --out-file ./dist/default-lib.es5.js",
    "transpile:umd": "babel --plugins=transform-es2015-modules-commonjs ./dist/bundles/default-lib.umd.js --out-file ./dist/bundles/default-lib.umd.js",
    "replace:html-dev": "htmlprocessor ./build/index.html -o ./build/index.html -e dev",
    "replace:html-prod": "htmlprocessor ./build/index.html -o ./build/index.html -e prod",
    "dev:server": "node server.js",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "lint": "tslint --force \"src/app/**/*.ts\"",
    "e2e": "protractor protractor.config.js",
    "e2e:live": "protractor protractor.config.js --elementExplorer",
    "pretest": "",
    "test": "karma start karma.conf.js",
    "test:watch": "karma start karma.conf.js --no-single-run --auto-watch",
    "ci": "npm run e2e && npm run test",
    "ci:watch": "npm run e2e && npm run test:watch",
    "start": "npm run build:dev",
    "build": "node build.js",
    "build:dev": "rimraf build && node build.dev.js",
    "build:custom": "rimraf build && node build.custom.js",
    "build:lib": "rimraf dist && node build.lib.js",
    "build:prod": "rimraf build && node build.prod.js",
    "serve": "node server.js",
    "postinstall": "npm run webdriver:update",
    "webpack": "webpack-dev-server"
  },
  "dependencies": {
    "@angular/animations": "4.3.6",
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/compiler-cli": "4.3.6",
    "@angular/core": "4.3.6",
    "@angular/forms": "4.3.6",
    "@angular/http": "4.3.6",
    "@angular/platform-browser": "4.3.6",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@angular/platform-server": "4.3.6",
    "@angular/router": "4.3.6",
    "angular": "git+https://github.com/angular/angular@4.3.6",
    "angular-in-memory-web-api": "~0.3.2",
    "commander": "^2.10.0",
    "core-js": "^2.4.1",
    "express": "^4.15.0",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "reflect-metadata": "~0.1.10",
    "rxjs": "~5.4.2",
    "shrink-ray": "^0.1.3",
    "systemjs": "0.20.14",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.53",
    "@types/karma": "0.13.36",
    "@types/node": "8.0.2",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "canonical-path": "0.0.2",
    "chalk": "^1.1.3",
    "chokidar": "^1.7.0",
    "clean-css": "^4.1.4",
    "clim": "^1.1.1",
    "codelyzer": "^3.1.1",
    "concurrently": "^3.5.0",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "google-closure-compiler": "^20170521.0.0",
    "google-closure-compiler-js": "^20170521.0.0",
    "html-minifier": "^3.5.2",
    "htmlprocessor": "^0.2.4",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^4.1.1",
    "js-string-escape": "^1.0.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "^0.3.5",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "^0.6.0",
    "karma-verbose-reporter": "^0.0.6",
    "live-server": "^1.2.0",
    "livereload": "^0.6.2",
    "node-sass": "^4.5.3",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss": "^6.0.3",
    "postcss-cli": "^4.1.0",
    "protractor": "^5.1.2",
    "remap-istanbul": "^0.9.5",
    "replace-in-file": "^2.5.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve-angular": "^2.0.3",
    "rollup-plugin-replace": "^1.1.1",
    "shelljs": "^0.7.8",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.4.3",
    "typescript": "^2.4.0"
  },
  "bin": {
    "ngr": "cli.js"
  }
}
